/** For building //packages/platform-browser. */
{
  "extends": "../tsconfig-tsec-base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".tsec",
    "paths": {
      "@angular/core": ["../core"],
      "@angular/common": ["../common"]
    }
  },
  "include": [
    "*.ts",
    "src/**/*.ts",
    "../*.d.ts"
  ],
  "references": [
    {"path": "../core/tsconfig-tsec.json"},
    {"path": "../common/tsconfig-tsec.json"},
    {"path": "../zone.js/lib/tsconfig-tsec.json"}
  ]
}
